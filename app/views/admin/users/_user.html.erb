<tr id="user-<%= user.id %>">
  <td><%= user.created_at.strftime Settings.index.time_format %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td></td>
  <td>
    <% if !current_user? user %>
      <%= link_to t(".delete"), admin_user_path(user.id),
                                method: :delete,
                                remote: true,
                                data: {confirm: t(".confirm_delete")} %>
    <% end %>
  </td>
</tr>
